<h1>Edit graphics</h1>

<%= simple_form_for [current_user, @reports, @graph_blocks] do |f| %>
<%= f.input :start_date %>
<%= f.input :end_date %>
<%= f.input :order %>
<%= f.input :graph_type %>
<%= f.button :submit %>
<% end %>

<%= link_to "Back to the report", user_report_path(current_user, @report) %>
